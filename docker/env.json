{
  "host_env_var_prefix": "INVEST",
  "file_path": "./docker/.env",
  "env_vars": {
    "required": [
      "PORT",
      "GUNICORN_TIMEOUT",
      "SECRET_KEY",
      "RECAPTCHA_PUBLIC_KEY",
      "RECAPTCHA_PRIVATE_KEY",
      "NOCAPTCHA",
      "ZENDESK_EMAIL",
      "ZENDESK_SUBDOMAIN",
      "ZENDESK_TOKEN",
      "IPSTACK_API_KEY",
      "GOOGLE_TAG_MANAGER_ID",
      "UTM_COOKIE_DOMAIN"
    ],
    "optional": [
      "ALLOWED_HOSTS",
      "SECURE_SSL_REDIRECT",
      "AWS_STORAGE_BUCKET_NAME",
      "AWS_ACCESS_KEY_ID",
      "AWS_SECRET_ACCESS_KEY",
      "AWS_S3_CUSTOM_DOMAIN",
      "CLOUDFRONT_DISTRIBUTION_ID",
      "STATIC_HOST",
      "REDIS_URL",
      "EMAIL_BACKEND_CLASS_NAME"
    ]
  }
}
